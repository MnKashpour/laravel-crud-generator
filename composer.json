{
    "name": "mnkashpour/laravel-crud-generator",
    "description": "Laravel package that generate files for crud",
    "license": "MIT",
    "authors": [
        {
            "name": "MnKashpour",
            "email": "mohanned.kashpour@gmail.com"
        }
    ],
    "homepage": "https://github.com/mnkashpour/laravel-crud-generator",
    "keywords": ["Laravel", "LaravelCrudGenerator", "MnKashpour"],
    "require": {
        "illuminate/support": "~10|~11"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "MnKashpour\\LaravelCrudGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MnKashpour\\LaravelCrudGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MnKashpour\\LaravelCrudGenerator\\LaravelCrudGeneratorServiceProvider"
            ]
        }
    }
}
